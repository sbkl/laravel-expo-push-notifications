{
    "name": "sbkl/laravel-expo-push-notifications",
    "description": "A Laravel wrapper around Alymosul/exponent-server-sdk-php",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Sebastien Koziel",
            "email": "sebastien@sbkl.ltd"
        }
    ],
    "require": {
        "alymosul/exponent-server-sdk-php": "^1.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.2",
        "orchestra/testbench": "^5.3"
    },
    "autoload": {
        "psr-4": {
            "Sbkl\\LaravelExpoPushNotifications\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Sbkl\\LaravelExpoPushNotifications\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Sbkl\\LaravelExpoPushNotifications\\ExpoServiceProvider"
            ],
            "aliases": {
                "Expo": "Sbkl\\LaravelExpoPushNotifications\\Facades\\Expo"
            }
        }
    }
}
